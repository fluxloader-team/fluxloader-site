{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "source": {
    "include": ["./"],
    "exclude": ["node_modules","docs"]
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme"
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
